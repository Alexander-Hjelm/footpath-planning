// Query to http://overpass-turbo.eu/

// Query for ways
<query type="way">
    <around lat="59.334591" lon="18.063240" radius="800"/>
    <has-kv k="highway"/>
</query>
<union>
  <item/>
  <recurse type="down"/>
</union>
<print/>

// Query for building footprints
<query type="wr">
    <around lat="59.334591" lon="18.063240" radius="800"/>
    <has-kv k="building"/>
</query>
<union>
  <item/>
  <recurse type="down"/>
</union>
<print/>

